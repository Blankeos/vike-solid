{
  "name": "vike-solid",
  "version": "0.2.7",
  "type": "module",
  "scripts": {
    "dev": "rollup -c rollup.config.js --watch",
    "build": "tsc --noEmit && rollup -c rollup.config.js",
    "release": "pnpm run build && bumpp --commit --push --tag && pnpm publish"
  },
  "dependencies": {
    "vite-plugin-solid": "^2.8.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8.7",
    "vite": "^4.4 || ^5.0.2",
    "vike": "^0.4.152"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^18.17.4",
    "babel-preset-solid": "^1.8.6",
    "bumpp": "^9.2.1",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "solid-js": "^1.8.7",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vike": "^0.4.153"
  },
  "exports": {
    ".": "./dist/+config.js",
    "./vite": "./dist/vite-plugin-vike-solid.js",
    "./usePageContext": "./dist/usePageContext.js",
    "./useData": "./dist/useData.js",
    "./ClientOnly": "./dist/ClientOnly.js",
    "./renderer/onRenderHtml": "./dist/+onRenderHtml.js",
    "./renderer/onRenderClient": "./dist/+onRenderClient.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/renderer/+config.d.ts"
      ],
      "vite": [
        "dist/vite-plugin-vike-solid.d.ts"
      ],
      "client": [
        "client.d.ts"
      ],
      "usePageContext": [
        "dist/components/usePageContext.d.ts"
      ],
      "useData": [
        "dist/components/useData.d.ts"
      ],
      "ClientOnly": [
        "dist/components/ClientOnly.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "client.d.ts"
  ],
  "bin": "./cli/entry.js",
  "main": "dist/+config.js",
  "types": "dist/renderer/+config.d.ts",
  "repository": "github:vikejs/vike-solid",
  "license": "MIT"
}
